include	PARM.lammps
pair_coeff 1 1 0.066047 3.4
bond_coeff 1 469 1.4
angle_coeff 1 63 120
dihedral_coeff 1 0 7.25 0 0
improper_coeff 1 5 180

# Simulation settings
group gcar type 1
680 atoms in group gcar
group gmid type 1   # define gmid
680 atoms in group gmid
group gtop type 1   # for later
680 atoms in group gtop
group gbot type 1   # for later
680 atoms in group gbot
variable xmax equal bound(gcar,xmax)-0.5
variable xmin equal bound(gcar,xmin)+0.5
region rtop block ${xmax} INF INF INF INF INF
region rtop block 38.796768 INF INF INF INF INF
region rbot block INF ${xmin} INF INF INF INF
region rbot block INF -0.728024 INF INF INF INF
region rmid block ${xmin} ${xmax} INF INF INF INF
region rmid block -0.728024 ${xmax} INF INF INF INF
region rmid block -0.728024 38.796768 INF INF INF INF

# Thermalisation and dynamics
velocity gmid create ${T} 48455 mom yes rot yes
velocity gmid create 300 48455 mom yes rot yes
fix mynve all nve
compute Tmid gmid temp
fix myber gmid temp/berendsen ${T} ${T} 100
fix myber gmid temp/berendsen 300 ${T} 100
fix myber gmid temp/berendsen 300 300 100
fix_modify myber temp Tmid

# restrain the motion of edges
group gtop type 1
680 atoms in group gtop
group gbot type 1
680 atoms in group gbot

fix mysf1 gtop setforce 0 NULL 0
fix mysf2 gbot setforce 0 NULL 0
velocity gtop set 0 NULL 0
velocity gbot set 0 NULL 0

# data extraction
variable	L equal xcm(gtop,x)-xcm(gbot,x)
fix 		at1 all ave/time 10 100 1000 v_L file length.dat
fix 		at2 all ave/time 10 100 1000 f_mysf1[1] f_mysf2[1] file force.dat
dump 		mydmp all atom 1000 dump.lammpstrj

# Equilibrium step
thermo 100
thermo_modify temp Tmid
WARNING: Temperature for thermo pressure is not for group all (src/thermo.cpp:527)

# Run

timestep 1.0
run 5000
